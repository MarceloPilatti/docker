{
  "basePath": "%%TERRAMA2_BASE_PATH%%%%WEBAPP_BASE_PATH%%",
  "port": 36000,
  "services": {
    "alert": {
      "host": "%%TERRAMA2_PROJECT_NAME%%_alert_1",
      "sshPort": 22,
      "sshUser": "terrama2"
    },
    "analysis": {
      "host": "%%TERRAMA2_PROJECT_NAME%%_analysis_1",
      "sshPort": 22,
      "sshUser": "terrama2"
    },
    "collector": {
      "host": "%%TERRAMA2_PROJECT_NAME%%_collector_1",
      "sshPort": 22,
      "sshUser": "terrama2"
    },
    "interpolator": {
      "host": "%%TERRAMA2_PROJECT_NAME%%_interpolator_1",
      "sshPort": 22,
      "sshUser": "terrama2"
    },
    "view": {
      "host": "%%TERRAMA2_PROJECT_NAME%%_view_1",
      "sshPort": 22,
      "sshUser": "terrama2",
      "geoserver": "%%TERRAMA2_PROTOCOL%%admin:geoserver@%%TERRAMA2_DNS%%:%%GEOSERVER_PORT%%%%TERRAMA2_BASE_PATH%%geoserver"
    }
  },
  "defaultFilePathList": [
    "/data",
    "/shared-data"
  ],
  "defaultExecutablePath": "/opt/terrama2/%%TERRAMA2_TAG%%/bin",
  "defaultExecutableName": "terrama2_service",
  "disablePDF": true,
  "debug": true,
  "OGR2OGR": "ogr2ogr",
  "SHP2PGSQL": "shp2pgsql",
  "toolsMenu": [
    {
      "textEn": "TerraMA² Homepage",
      "textEs": "TerraMA² Homepage",
      "textFr": "TerraMA² Homepage",
      "textPt": "TerraMA² Homepage",
      "url": "http://www.terrama2.dpi.inpe.br/",
      "openInNewTab": true
    }
  ]
}
